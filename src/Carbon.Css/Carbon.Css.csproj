<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Carbon.Css</AssemblyName>
    <VersionPrefix>0.44.0</VersionPrefix>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>true</WarningsAsErrors>

    <!-- Package Properties-->
    <Authors>iamcarbon</Authors>
    <Description>Fast (S)CSS parser and auto-prefixer.</Description>
    <Copyright>© 2012-2021 Jason Nelson</Copyright>
    <PackageTags>css;scss;autoprefix</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/carbon/css</RepositoryUrl>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Carbon.Color" Version="2.27.0" />
  </ItemGroup>

</Project>