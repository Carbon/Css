<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Carbon.Css</AssemblyName>
    <VersionPrefix>0.42.0</VersionPrefix>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5.0</TargetFrameworks>
    <LangVersion>9</LangVersion>
    <Nullable>enable</Nullable>

    <!-- Package Properties-->
    <Authors>iamcarbon</Authors>
    <Description>High-performance (S)CSS parser and auto-prefixer.</Description>
    <Copyright>© 2012-2020 Jason Nelson</Copyright>
    <PackageId>Carbon.Css</PackageId>
    <PackageTags>css;scss;autoprefix</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/carbon/css</RepositoryUrl>
    <AutoGenerateBindingRedirects>false</AutoGenerateBindingRedirects>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Carbon.Color" Version="2.24.0" />
  </ItemGroup>

</Project>