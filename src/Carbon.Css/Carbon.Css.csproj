<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Carbon.Css</AssemblyName>
    <VersionPrefix>0.8.0</VersionPrefix>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    
    <!-- Package Properties-->
    <Authors>iamcarbon</Authors>
    <Description>High-performance netstandard CSS parser and auto-prefixer.</Description>
    <Copyright>© 2012-2017 Jason Nelson</Copyright>
    <PackageTags>css;scss;autoprefix</PackageTags>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/carbon/css</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Carbon.Color" Version="1.3.0" />
  </ItemGroup>

</Project>
