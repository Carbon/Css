<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Carbon.Css</AssemblyName>
    <TargetFrameworks>net5.0;net6.0</TargetFrameworks>
    <LangVersion>10</LangVersion>
    <Version>0.51.0</Version>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>true</WarningsAsErrors>

    <!-- Package Properties-->
    <Authors>iamcarbon</Authors>
    <Description>Fast (S)CSS parser and auto-prefixer.</Description>
    <Copyright>© 2012-2021 Jason Nelson</Copyright>
    <PackageTags>css;scss;autoprefix</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/carbon/css</RepositoryUrl>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Carbon.Color" Version="2.29.0" />
  </ItemGroup>

</Project>